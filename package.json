{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "start": "node index.js",
    "start:watch": "nodemon",
    "prestart:prod": "tsc",
    "start:prod": "node dist/main.js",
    "test": "jest --forceExit --coverage",
    "test:watch": "jest --watch",
    "e2e": "jest --config=e2e/jest-e2e.json --forceExit"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/src/.*\\.(spec).(ts|tsx|js)$",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,tsx,ts}",
      "!**/node_modules/**"
    ],
    "mapCoverage": true,
    "coverageReporters": [
      "json",
      "lcov"
    ]
  },
  "dependencies": {
    "@nestjs/common": "^4.5.9",
    "@nestjs/core": "^4.5.10",
    "@nestjs/microservices": "^4.5.8",
    "@nestjs/mongoose": "^3.0.1",
    "@nestjs/swagger": "^1.1.4",
    "@nestjs/testing": "^4.5.5",
    "@nestjs/websockets": "^4.4.0",
    "await-to-js": "^2.0.1",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "facebook-nodejs-ads-sdk": "^2.11.2",
    "jsonwebtoken": "^8.1.0",
    "mongoose": "^5.0.3",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.1",
    "redis": "^2.7.1",
    "reflect-metadata": "^0.1.10",
    "request": "^2.83.0",
    "chalk": "^2.3.0",
    "rxjs": "^5.4.0",
    "typescript": "^2.5.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^1.0.0",
    "@types/jest": "^22.1.0",
    "@types/jsonwebtoken": "^7.2.5",
    "@types/mongoose": "^5.0.0",
    "@types/node": "^9.4.0",
    "@types/supertest": "^2.0.4",
    "@types/express": "^4.11.0",
    "@types/passport-jwt": "^3.0.0",
    "jest": "^22.1.4",
    "mockgoose-fix": "^7.3.6",
    "mongoose-model-mock": "^1.4.0",
    "nodemon": "^1.14.11",
    "supertest": "^3.0.0",
    "ts-jest": "^22.0.1",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1"
  }
}
