{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "start": "node index.js",
    "start:watch": "nodemon --trace-warnings",
    "build": "tsc",
    "predockerize": "npm run lint && npm run build",
    "dockerize": "./config/dockerize.sh",
    "start:prod": "node dist/main.js",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "e2e": "jest --config=e2e/jest-e2e.json --forceExit"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/src/.*\\.(spec).(ts|tsx|js)$",
    "collectCoverageFrom": [
      "!src/**/*types.{js,jsx,tsx,ts}",
      "src/**/*.{js,jsx,tsx,ts}",
      "!**/node_modules/**"
    ],
    "mapCoverage": true,
    "verbose": true,
    "coverageReporters": [
      "json",
      "lcov"
    ]
  },
  "dependencies": {
    "@nestjs/common": "^4.6.6",
    "@nestjs/core": "^4.6.6",
    "@nestjs/microservices": "^4.6.6",
    "@nestjs/mongoose": "^3.0.1",
    "@nestjs/swagger": "^1.1.4",
    "@nestjs/testing": "^4.6.6",
    "@nestjs/websockets": "^4.6.6",
    "await-to-js": "^2.0.1",
    "bcrypt": "^1.0.3",
    "bunyan": "^1.8.12",
    "chalk": "^2.3.0",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "moment": "^2.22.1",
    "mongoose": "^5.0.3",
    "morgan": "^1.9.0",
    "npm": "^6.0.0",
    "passport": "^0.4.0",
    "passport-facebook-token": "^3.3.0",
    "passport-google-plus-token": "^2.1.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-twitter-token": "^1.3.0",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.12",
    "request": "^2.83.0",
    "rxjs": "^5.5.10",
    "typescript": "^2.5.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@types/bcrypt": "^1.0.0",
    "@types/bunyan": "^1.8.4",
    "@types/express": "^4.11.0",
    "@types/jest": "^22.1.0",
    "@types/jsonwebtoken": "^7.2.5",
    "@types/lodash": "^4.14.92",
    "@types/moment": "^2.13.0",
    "@types/mongoose": "^5.0.0",
    "@types/passport": "^0.4.5",
    "@types/passport-local": "^1.0.33",
    "@types/node": "^9.4.0",
    "@types/passport-jwt": "^3.0.1",
    "@types/supertest": "^2.0.4",
    "jest": "^22.1.4",
    "mockgoose-fix": "^7.3.6",
    "mongoose-model-mock": "^1.4.0",
    "nodemon": "^1.14.11",
    "supertest": "^3.0.0",
    "ts-jest": "^22.0.1",
    "ts-node": "^4.1.0",
    "tsconfig-paths": "^3.1.1",
    "tslint": "^5.9.1"
  }
}
